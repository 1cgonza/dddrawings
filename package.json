{
  "name": "ddd",
  "version": "1.0.0",
  "description": "Data Driven Drawings",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/1cgonza/dddrawings"
  },
  "scripts": {
    "dev": "wait-on http://localhost:9000/ && babel-node app.js",
    "pack": "webpack-dev-server --config webpack.dev.js",
    "start": "concurrently --kill-others \"yarn run pack\" \"yarn run dev\"",
    "images": "babel-node app --img",
    "clean": "babel-node app --clean",
    "pack:prod": "webpack --config webpack.prod.js",
    "build": "yarn pack:prod && babel-node app --prod",
    "deploy": "yarn pack:prod && babel-node app --deploy"
  },
  "keywords": [
    "PhD",
    "Experiments",
    "Data",
    "Drawings"
  ],
  "author": "Juan Camilo Gonzalez <info@juancgonzalez.com> (http://juancgonzalez.com)",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/1cgonza/dddrawings/issues"
  },
  "homepage": "https://github.com/1cgonza/dddrawings",
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "d3": "^5.14.2",
    "dddrawings": "^1.0.2",
    "nouislider": "^14.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "connect-gzip-static": "^2.1.1",
    "del": "^5.1.0",
    "flatted": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulpsmith": "^0.6.0",
    "handlebars": "^4.7.7",
    "jstransformer-handlebars": "^1.1.0",
    "metalsmith": "^2.3.0",
    "metalsmith-autoprefixer": "^1.1.2",
    "metalsmith-better-excerpts": "^0.2.1",
    "metalsmith-changed": "^3.1.1",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-debug": "^1.2.0",
    "metalsmith-discover-helpers": "^0.1.1",
    "metalsmith-discover-partials": "^0.1.2",
    "metalsmith-drafts": "^1.0.0",
    "metalsmith-each": "^0.1.1",
    "metalsmith-html-minifier": "^4.0.1",
    "metalsmith-ignore": "^1.0.0",
    "metalsmith-layouts": "^2.3.1",
    "metalsmith-markdown": "^1.3.0",
    "metalsmith-metadata": "^0.0.4",
    "metalsmith-sass": "^1.6.0",
    "node-sass": "^7.0.1",
    "sharp": "^0.30.5",
    "slug": "^2.1.1",
    "striptags": "^3.2.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
